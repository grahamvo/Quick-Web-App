{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "airbnb",
    "rules": {
        "indent": [2, 4],
        "react/jsx-indent": [2, 4],
        "react/jsx-curly-spacing": [2, "always"],
        "arrow-body-style": [2, "always"],
        "no-underscore-dangle": 0,
        "react/jsx-indent-props": [2, 4],
        "react/jsx-closing-bracket-location": 0,
        "import/export": 0,
    },
    "plugins": [
        "react",
    ]
}
